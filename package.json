{
  "name": "webilastik-overlay",
  "version": "1.0.0",
  "description": "lastik web frontend overlay",
  "main": "build/cjs/index.js",
  "types": "build/cjs/index.d.ts",
  "scripts": {
    "build-cjs": "rm -rfv build/cjs && npx tsc --outDir build/cjs --module CommonJS",
    "build": "npm run build-cjs",
    "bundle": "npm run build && npx esbuild src/demo.ts --bundle --loader:.ts=ts --sourcemap --outfile=build/bundle.js",
    "package": "npm run bundle && npm pack"
  },
  "files": [
    "build/**/*"
  ],
  "author": "Tomaz Vieira",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.11.15",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "gl-matrix": "^3.3.0"
  }
}
